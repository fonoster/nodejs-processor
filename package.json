{
  "name": "nodejs-processor",
  "version": "0.2.1",
  "description": "Routr Processor Template",
  "main": "dist/src/index.js",
  "homepage": "https://github.com/fonoster/nodejs-processor#readme",
  "author": "Fonoster Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fonoster/nodejs-processor/issues"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=dev nodemon src/index.ts",
    "test": "cross-env NODE_ENV=dev nyc --reporter=lcov mocha test/*.test.ts --exit",
    "prebuild": "rimraf ./dist tsconfig.tsbuildinfo",
    "build": "tsc -b tsconfig.json",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "gen:docs": "node .scripts/docs-generator.js && cp docs/service.md",
    "prepare": "husky install"
  },
  "bin": {
    "runner": "./dist/src/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "./src/"
    ],
    "ext": "ts",
    "exec": "ts-node",
    "ignore": "**/*.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fonoster/nodejs-processor.git"
  },
  "keywords": [
    "template"
  ],
  "directories": {
    "src": "src",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fonoster/logger": "^0.15.20",
    "@routr/location": "^2.13.13",
    "@routr/processor": "^2.13.13",
    "fp-ts": "^2.16.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/chai": "^4.3.14",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.8",
    "@types/node": "^24.9.2",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "chai": "^4.3.14",
    "chai-as-promised": "^7.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.38.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-notice": "^1.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "esm": "^3.2.25",
    "husky": "^9.1.7",
    "jsdoc-to-markdown": "^8.0.1",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.9",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "sinon": "^17.0.2",
    "sinon-chai": "^3.7.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
